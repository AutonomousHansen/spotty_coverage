. ./defs

set -e

run 1 ../checker 'a'
run 1 ../checker 'a U b'
run 1 ../checker 'X a'
run 1 ../checker 'a & b & c'
run 1 ../checker 'a | b | (c U (d & (g U (h ^ i))))'
run 1 ../checker 'Xa & (b U !a) & (b U !a)'
run 1 ../checker 'Fa & Xb & GFc & Gd'
run 1 ../checker 'Fa & Xa & GFc & Gc'
run 1 ../checker 'Fc & X(a | Xb) & GF(a | Xb) & Gc'
run 1 ../checker '!((FF a) <=> (F x))'
run 0 ../checker 'Xa && (!a U b) && !b && X!b'
run 0 ../checker '(a U !b) && Gb'
for i in $(seq 1 100); do
  run 0 ../checker -rndgraph  'a|b|c|d|e|f|g|h|i|k|l'
done
