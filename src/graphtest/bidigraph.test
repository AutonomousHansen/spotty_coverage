. ./defs

set -e

cat << EOF > a.ref
digraph G {
  1 -> 2
  2 -> 2
}
EOF

run 0 ../bidigraph a > a.out
cmp a.out a.ref

cat << EOF > xa.ref
digraph G {
  1 -> 2
  2 -> 3
  3 -> 3
}
EOF
run 0 ../bidigraph Xa > xa.out
cmp xa.out xa.ref

cat << EOF > b.ref
digraph G {
  1 -> 2
  1 -> 3
  1 -> 4
  2 -> 2
  3 -> 2
  3 -> 3
  4 -> 2
  4 -> 3
  4 -> 4
}
EOF
run 0 ../bidigraph "Xa Ub R c" > b.out
cmp b.out b.ref
