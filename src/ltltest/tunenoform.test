#! /bin/sh

# Check for unabbreviate_ltl + negative_normal_form visitors

. ./defs || exit 1

set -e

./tunenoform '!(a ^ b)' '(a|!b) & (!a|b)'
./tunenoform '!(a <=> b)' '(a|b) & (!a|!b)'
./tunenoform '!(a => b)' 'a&!b'
./tunenoform '!(!a => !b)' '!a&b'
./tunenoform '!Fa' 'false R !a'
./tunenoform '!G!a' 'true U a'
./tunenoform '!(GF a => FG b)' '(0 R (1 U a)) & (0 R (1 U !b))'
