# Check for the reduc visitor

. ./defs || exit 1

#

run 1 ./inf '1' '1'
run 1 ./inf '0' '0'

run 1 ./inf 'a' '1'
run 1 ./inf 'a' 'a'

run 1 ./inf 'a' 'a * 1'

run 1 ./inf 'a * b' 'b'
run 1 ./inf 'a * b' 'a'

run 1 ./inf 'a' 'a + b'
run 1 ./inf 'b' 'a + b'

run 1 ./inf 'a + b' '1'

run 1 ./inf 'a' 'b U a'
run 1 ./inf 'a' 'b U 1'
run 1 ./inf 'a U b' '1'

run 1 ./inf 'a' '1 R a'
run 1 ./inf 'a' 'a R 1'
run 1 ./inf 'a R b' 'b'
run 1 ./inf 'a R b' '1'

run 1 ./inf 'a U b' '1 U b'
run 1 ./inf 'a R b' '1 R b' 

run 1 ./inf 'b * (a U b)' 'a U b'
run 1 ./inf 'a U b' 'c + (a U b)'

exit 0
#

#run 1 ./inf '(a U b) U ((a U b) U (a U b))' 'a U b'
#run 1 ./inf '(a U b) && (a U b)' 'a U b'

'X1' '1'
'a U 0' '0'
'a R 1' '1'
'Xa * Xb' 'X(a * b)'
'F(a * GFb)' 'Fa * GFb'
'G(a + GFb)' 'Ga + GFb'
'X(a * GFb)' 'Xa * GFb'
'X(a + GFb)' 'Xa + GFb'
