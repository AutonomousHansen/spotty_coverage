. ./defs

set -e

run 0 ../explicit3 | tee stdout
cat >expected <<EOF
digraph G {
  0 [label="", style=invis, height=0]
  0 -> 1
  1 [label="0"]
  1 -> 2 [label="a\n"]
  2 [label="1"]
  2 -> 3 [label="a\n"]
  2 -> 4 [label="b\n"]
  3 [label="2"]
  3 -> 1 [label="a\n"]
  4 [label="3"]
  4 -> 5 [label="c\n"]
  5 [label="4"]
  5 -> 4 [label="c\n{Acc[b]}"]
}
digraph G {
  0 [label="", style=invis, height=0]
  0 -> 1
  1 [label="0"]
  1 -> 2 [label="a\n"]
  2 [label="1"]
  2 -> 3 [label="a\n"]
  3 [label="2"]
  3 -> 1 [label="a\n"]
}
digraph G {
  0 [label="", style=invis, height=0]
  0 -> 1
  1 [label="0"]
}
digraph G {
  0 [label="", style=invis, height=0]
  0 -> 1
  1 [label="1"]
  1 -> 2 [label="b\n"]
  2 [label="3"]
  2 -> 3 [label="c\n"]
  3 [label="4"]
  3 -> 2 [label="c\n{Acc[b]}"]
}
EOF

cmp expected stdout
