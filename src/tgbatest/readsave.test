#!/bin/sh

. ./defs

set -e

cat >input <<EOF
s1, "s2", a!b, c d;
"s2", "state 3", a, !c;
"state 3", s1,,;
EOF

./readsave input > stdout

cat >expected <<EOF
"s1", "s2", a !b, "c" "d";
"s2", "state 3", a, !"c";
"state 3", "s1", , ;
EOF

diff stdout expected

mv stdout input
./readsave input > stdout
diff input stdout

rm input stdout expected
