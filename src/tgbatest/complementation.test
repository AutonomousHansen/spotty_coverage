set -e

while read f; do
    run 0 ./complement -f "$f"
done <<EOF
GFa
FGa
<>p1->(p0Up1)
[](p0-><>p3)
GFa&&FGa
[] ((p2 && ! p1) -> (p0 U (p1 || [] p0)))
[] (p2 -> ((! p0 && ! p1) U (p1 || ((p0 && ! p1) U (p1 || ((! p0 && ! p1) \
 U (p1 || ((p0 && ! p1) U ((p1 || (! p0 U (p1 || [] ! p0))) || [] p0)))))))))
(p0 U (p1 && X (p2 && (true U (p3 && X (true U (p4 && X \
(true U (p5 && X (true U p6))))))))))
EOF
