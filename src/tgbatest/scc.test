. ./defs

set -e

cat >formulas<<EOF
((a U c) U b) & (b U c),15,6,5
b U a,3,2,2
0,0,1,1
(Gb | F!a) W GFc,22,6,5
EOF

run 0 ../../bin/ltl2tgba --low --any --stats='%f,%e,%s,%c' -F formulas/1 >out
cat out
diff out formulas
