. ./defs

set -e

for i in 0 1 2 3 4; do
  ../../bin/randaut a --seed=$i -S4 --hoa >iso$i
  ../../bin/autfilt iso$i --randomize --hoa >aut$i
done

cat aut0 aut1 aut2 aut3 aut4 > all
(for i in 0 1 2 3 4; do
  run 0 ../../bin/autfilt all --are-isomorphic iso$i --hoa
done) > output
diff all output
