. ./defs

set -e

../../bin/randaut a b -S10 --hoa >filt

randomize()
{
  for i in `seq 1 5`
  do
    ../../bin/autfilt --seed=$i --randomize=$1 -F filt --hoa >> autiso
  done
}

randomize s
randomize t
randomize

run 0 ../../bin/autfilt -F autiso --isomorph filt --hoa >out
test `grep HOA out | wc -l` -eq 15

cat >notiso <<EOF
