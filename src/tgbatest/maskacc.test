. ./defs

set -e

cat >input1 <<EOF
acc = A B;
s1, s2, "a", A;
s1, s3, "b", B;
s2, s3, "a",;
s3, s2, "b",;
s2, s4, "a", B;
s3, s4, "b", A;
s4, s4, "a", A B;
EOF

cat >expect1 <<EOF
acc = "0" "1";
"0", "2", "b", "1";
"2", "1", "b",;
"1", "2", "a",;
"1", "3", "a", "1";
acc = "0" "1";
"0", "1", "a", "0";
"1", "2", "a",;
"2", "1", "b",;
"2", "3", "b", "0";
EOF

run 0 ../maskacc input1 | tee stdout
diff stdout expect1
