. ./defs

set -e

run 0 ../tgba_analysis -r1 -dha '( XX b & (XXG a | XXGF b | XX c))' >stdout
cat >expected <<EOF
init:G,Terminal:1,Weak:1,General:4,commut:1,states:6,trans:9,tgg:5,tgw:1,tgt:1,tww:1,twt:0,ttt:1

EOF
diff stdout expected


run 0 ../tgba_analysis -r1 -dha '(Ga -> G b) W c' >stdout
cat >expected <<EOF
init:G,Terminal:2,Weak:1,General:2,commut:1,states:5,trans:12,tgg:4,tgw:1,tgt:3,tww:1,twt:0,ttt:3

EOF
diff stdout expected

run 0 ../tgba_analysis -dha '(GFa R b)' >stdout
cat >expected <<EOF
init:G,Terminal:0,Weak:0,General:2,commut:0,states:2,trans:4,tgg:4,tgw:0,tgt:0,tww:0,twt:0,ttt:0

EOF
diff stdout expected
