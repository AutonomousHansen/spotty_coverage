. ./defs

set -e

run 0 ../tgba_analysis -r1 -m -dta '( XX b & (XXG a | XXGF b | XX c))' >stdout
cat >expected <<EOF
digraph G {
  0 [label="", style=invis, height=0]
  0 -> 1
  1 [label="3"]
  1 -> 2 [label="1\n{Acc[1]}"]
  2 [label="1"]
  2 -> 3 [label="1\n{Acc[1]}"]
  3 [label="0"]
  3 -> 4 [label="b & c\n{Acc[1]}"]
  4 [label="2"]
  4 -> 4 [label="1\n{Acc[1]}"]
}
EOF
diff stdout expected

run 0 ../tgba_analysis -r1 -m -dwa '( XX b & (XXG a | XXGF b | XX c))' >stdout
cat >expected <<EOF
digraph G {
  0 [label="", style=invis, height=0]
  0 -> 1
  1 [label="4"]
  1 -> 2 [label="1\n"]
  2 [label="3"]
  2 -> 3 [label="1\n"]
  3 [label="2"]
  3 -> 4 [label="b & a & !c\n"]
  4 [label="1"]
  4 -> 4 [label="a\n{Acc[1]}"]
}
EOF
diff stdout expected

run 0 ../tgba_analysis -r1 -m -dsa '( XX b & (XXG a | XXGF b | XX c))' >stdout
cat >expected <<EOF
digraph G {
  0 [label="", style=invis, height=0]
  0 -> 1
  1 [label="4"]
  1 -> 2 [label="1\n"]
  2 [label="1"]
  2 -> 3 [label="1\n"]
  3 [label="2"]
  3 -> 4 [label="b & !c\n"]
  4 [label="3"]
  4 -> 4 [label="b\n{Acc[b]}"]
  4 -> 4 [label="!b\n"]
}
EOF
diff stdout expected

run 0 ../tgba_analysis -r1 -m -dta '(Ga -> G b) W c' >stdout
cat >expected <<EOF
digraph G {
  0 [label="", style=invis, height=0]
  0 -> 1
  1 [label="1"]
  1 -> 2 [label="c\n"]
  1 -> 3 [label="a & !c\n"]
  1 -> 1 [label="!a & !c\n"]
  2 [label="3"]
  2 -> 2 [label="1\n{Acc[1]}"]
  3 [label="2"]
  3 -> 4 [label="a & c\n"]
  3 -> 2 [label="c & !a\n"]
  3 -> 3 [label="a & !c\n"]
  3 -> 1 [label="!a & !c\n"]
  4 [label="0"]
  4 -> 4 [label="a\n"]
  4 -> 2 [label="!a\n"]
}
EOF
diff stdout expected

run 0 ../tgba_analysis -r1 -m -dwa '(Ga -> G b) W c' >stdout
cat >expected <<EOF
digraph G {
  0 [label="", style=invis, height=0]
  0 -> 1
  1 [label="2"]
  1 -> 2 [label="b & !c\n"]
  1 -> 3 [label="a & !c\n"]
  1 -> 1 [label="!a & !c\n"]
  2 [label="1"]
  2 -> 2 [label="b\n{Acc[1]}"]
  3 [label="3"]
  3 -> 1 [label="!a & !c\n"]
  3 -> 3 [label="a\n"]
}
EOF
diff stdout expected

run 0 ../tgba_analysis -r1 -m -dsa '(Ga -> G b) W c' >stdout
cat >expected <<EOF
digraph G {
  0 [label="", style=invis, height=0]
  0 -> 1
  1 [label="2"]
  1 -> 2 [label="a & !c\n"]
  1 -> 1 [label="!a & !c\n{Acc[!a]}"]
  2 [label="1"]
  2 -> 1 [label="!a & !c\n{Acc[!a]}"]
  2 -> 2 [label="a\n"]
}
EOF
diff stdout expected

run 0 ../tgba_analysis -m -dta '(GFa R b)' 2>/dev/null >stdout
cat >expected <<EOF
EOF
diff stdout expected

run 0 ../tgba_analysis -m -dwa '(GFa R b)' >stdout
cat >expected <<EOF
digraph G {
  0 [label="", style=invis, height=0]
  0 -> 1
  1 [label="1"]
  1 -> 1 [label="b\n"]
}
EOF
diff stdout expected

run 0 ../tgba_analysis -m -dsa '(GFa R b)' >stdout
cat >expected <<EOF
digraph G {
  0 [label="", style=invis, height=0]
  0 -> 1
  1 [label="2"]
  1 -> 2 [label="b\n"]
  1 -> 1 [label="b\n{Acc[a]}"]
  2 [label="1"]
  2 -> 2 [label="a\n{Acc[a]}"]
  2 -> 2 [label="!a\n"]
}
EOF
diff stdout expected
