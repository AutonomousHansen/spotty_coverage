. ./defs
set -e

run 0 ../powerset > out
cat >exp <<EOF
==P({})==
{  }
==P({0})==
{  }
{ 0 }
==P({0, 1})==
{  }
{ 0 }
{ 0, 1 }
{ 1 }
==P({0, 1, 2})==
{  }
{ 0 }
{ 0, 1 }
{ 1 }
{ 1, 2 }
{ 0, 1, 2 }
{ 0, 2 }
{ 2 }
==P({0, 1, 2, 3})==
{  }
{ 0 }
{ 0, 1 }
{ 1 }
{ 1, 2 }
{ 0, 1, 2 }
{ 0, 2 }
{ 2 }
{ 2, 3 }
{ 0, 2, 3 }
{ 0, 1, 2, 3 }
{ 1, 2, 3 }
{ 1, 3 }
{ 0, 1, 3 }
{ 0, 3 }
{ 3 }
EOF

diff exp out
