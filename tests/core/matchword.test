../matchword '| a b' 'a&b;a&b;a&b;a&b;cycle{a&b;a&b}' '1'
../matchword '| a b' 'b;a&b;a&b;a&b;cycle{a&b;a&b}' '1'
../matchword '| a b' 'c;a&b;a&b;a&b;cycle{a&b;a&b}' '0'
